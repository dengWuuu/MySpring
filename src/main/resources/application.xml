<?xml version="1.0" encoding="UTF-8"?>
<beans>

    <bean id="helloWorldService" class="org.spring.HelloWorldServiceImpl" scope="prototype">
        <property name="text" value="Hello World"/>
    </bean>

    <bean id="wrapService" class="org.spring.WrapService">
        <property name="helloWorldService" ref="helloWorldService"/>
    </bean>

</beans>